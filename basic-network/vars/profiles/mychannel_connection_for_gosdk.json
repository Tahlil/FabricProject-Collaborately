      ]
    },
    "org1.example.com": {
      "mspid": "org1-example-com",
      "cryptoPath": "org1.example.com/users/{username}@org1.example.com/msp",
      "peers": [
        "peer1.org1.example.com",
        "peer2.org1.example.com"
      ],
      "orderers": [
      ]
    }
  },
  "orderers": {
    "orderer1.example.com": {
      "url": "grpcs://orderer1.example.com:7050",
      "grpcOptions": {
        "ssl-target-name-override": "orderer1.example.com"
      },
      "tlsCACerts": {
        "path": "/keyfiles/ordererOrganizations/example.com/orderers/orderer1.example.com/tls/ca.crt"
      }
    },
    "orderer2.example.com": {
      "url": "grpcs://orderer2.example.com:7050",
